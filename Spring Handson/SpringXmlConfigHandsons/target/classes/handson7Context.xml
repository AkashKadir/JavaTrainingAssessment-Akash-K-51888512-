<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="user" class="org.handson7.User">
		<property name="name" value="Jegan"/>
		<property name="age" value="24"/>
		<property name="city" value="Chennai"/>
		<property name="order" ref="orderOne"/>
	</bean>
	
	<bean id="orderOne" class="org.handson7.Order">
		<property name="itemName" value="Item1"/>
		<property name="price" value="230.07"/>
	</bean>
	
	<bean id="orderTwo" class="org.handson7.Order">
		<property name="itemName" value="Item2"/>
		<property name="price" value="230.07"/>
	</bean>

</beans>
